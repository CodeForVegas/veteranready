{{!< default}}
<div class="container">
  <div class="jumbotron">
    <div class="container text-center" style="margin-top:-35px; margin-bottom:-30px">
			<h2 class="text-center">Showing {{data.agency.length}} Community Partners</h2>
    </div>
  </div>
</div>
<div class="container">
	<div class="container text-center">
    {{#each data.agency}}
    <h2>{{name}}</h2>
		<div class="container" style="margin-top:5%;">
			<div class="col-md-6">
				{{#if video}}
          <div class="responsiveIframe">
            <iframe src="{{video}}" allowfullscreen></iframe>
          </div>
        {{/if}}<br />
        <div class="thumbnail col-md-8 col-md-offset-2">
  				Contact Name: {{contactName}}<br />
  				{{#if contactPhoto}}<img src="{{cloudinaryUrl contactPhoto width='400'}}" class="img-responsive" style="margin-top:10px;">{{/if}}<br />
          Contact Title: {{contactTitle}}<br />
  				Contact Military Service: {{contactMilitaryService}}<br />
        </div>
			</div>
			<div class="col-md-6">
        <div class="col-md-8 col-md-offset-2">
          {{#if logo}}<img src="{{cloudinaryUrl logo width='400'}}" class="img-responsive">{{/if}}<br />
        </div>
        <div class="thumbnail right-caption span6">
          <div class="caption">
      			Website: {{website}}<br />
      			Email: {{email}}<br />
            Address: {{address}}<br />
      			Phone Number: {{phone}}<br />
      			Social Media: {{socialMedia}}<br />
      			Summary: {{summary}}<br />
      			Description: {{description}}<br />
      			Associated Agencies: {{associateAgencies}}<br />
      			Topic: {{topic}}<br />
      			Keywords: {{keywords}}<br />
          </div>
        </div>
			</div>
		</div>
    {{/each}}
	</div>
	{{>pagination}}
</div>
