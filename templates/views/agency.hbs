{{!< default}}
<div class="container">
  <div class="jumbotron">
    <div class="container text-center" style="margin-top:-35px; margin-bottom:-30px">
			<h2 class="text-center">Showing {{data.agency.length}} Community Partners</h2>
    </div>
  </div>
</div>
<div class="container">
	{{#each data.agency}}
		<div class="container text-center">
      <h2>{{name}}</h2>
			<div class="container" style="margin-top:5%;">
				<div class="col-md-6">
					{{#if logo}}<img src="{{cloudinaryUrl logo width='300'}}" class="img-responsive">{{/if}}<br />
					{{#if video}}<iframe width="300" height="220" src="{{video}}" frameborder="0" allowfullscreen></iframe>{{/if}}<br />
					Contact Name: {{contactName}}<br />
					Contact Title: {{contactTitle}}<br />
					Contact Military Service: {{contactMilitaryService}}<br />
					{{#if contactPhoto}}<img src="{{cloudinaryUrl contactPhoto width='300'}}" class="img-responsive">{{/if}}<br />
				</div>
				<div class="col-md-6">
					Website: {{website}}<br />
					Email: {{email}}<br />
		      Address: {{address}}<br />
					Phone Number: {{phone}}<br />
					Social Media: {{socialMedia}}<br />
					Summary: {{summary}}<br />
					Description: {{description}}<br />
					Associated Agencies: {{associateAgencies}}<br />
					Topic: {{topic}}<br />
					Keywords: {{keywords}}<br />
				</div>
			</div>
		{{/each}}
	</div>
	{{>pagination}}
</div>
