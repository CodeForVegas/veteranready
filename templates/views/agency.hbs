{{!< default}}
<div class="container">
  <div class="jumbotron">
    <div class="container text-center" style="margin-top:-35px; margin-bottom:-30px">
			<h2 class="text-center">Showing {{data.agency.length}} Community Partners</h2>
    </div>
  </div>
</div>
<div class="container">
	<div class="container text-center">
    {{#each data.agency}}
    <h2>{{name}}</h2>
    <h3><a target="_blank" href="{{website}}">{{website}}</a></h3>
		<div class="container" style="margin-top:5%;">
			<div class="col-md-6">
				{{#if video}}
          <div class="responsiveIframe">
            <iframe src="{{video}}" allowfullscreen></iframe>
          </div>
        {{/if}}<br />
        <div class="thumbnail col-md-8 col-md-offset-2">
  				<h3>{{contactName}}</h3>
  				{{#if contactPhoto}}
            <img src="{{cloudinaryUrl contactPhoto width='400'}}" class="img-responsive" style="margin-top:10px;">
          {{/if}}
          <h3>{{contactTitle}}</h3>
  				{{#if contactMilitaryService}}
            <h3>{{contactMilitaryService}}</h3>
          {{/if}}
        </div>
			</div>
			<div class="col-md-6">
        <div class="col-md-8 col-md-offset-2" style="margin-bottom:5%;">
          {{#if logo}}
            <img src="{{cloudinaryUrl logo width='400'}}" class="img-responsive">
          {{/if}}
        </div>
        <div class="thumbnail right-caption span6">
          <div class="caption">
      			<p>Email: {{email}}</p>
            <p>Address: {{address}}</p>
      			<p>Phone Number: {{phone}}</p>
      			<p>Social Media: {{socialMedia}}</p>
      			<p>Summary: {{summary}}</p>
      			<p>Description: {{description}}</p>
      			<p>Associated Agencies: {{associateAgencies}}</p>
      			<p>Topic: {{topic}}</p>
      			<p>Keywords: {{keywords}}</p>
          </div>
        </div>
			</div>
		</div>
    {{/each}}
	</div>
	{{>pagination}}
</div>
