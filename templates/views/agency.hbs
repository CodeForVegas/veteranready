<div class="container">
</div>
<div class="container">
	<div class="row">
		<div class="col-sm-8 col-md-9">
			<h4 class="text-weight-normal">Showing {{data.agency.length}} Community Partners.</h4>
			<div class="blog">
				{{#each data.agency}}
				<div class="post">
          <h2>{{name}}</h2>
          email: {{email}}<br />
					topic: {{topic}}<br />
					{{#if contactPhoto}}<img src="{{cloudinaryUrl contactPhoto width='300' height='300'}}" class="img-rounded">{{/if}}<br />
          {{#if video}}<iframe width="420" height="315" src="{{video}}" frameborder="0" allowfullscreen></iframe>{{/if}}<br />
				</div>
				{{/each}}
			</div>
		</div>
	</div>
	{{>pagination}}
</div>
